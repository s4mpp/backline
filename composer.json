{
    "name": "s4mpp/backline",
    "description": "Modern and modular Admin Panel for Laravel",
    "keywords": [
        "s4mpp",
        "backline"
    ],
    "homepage": "https://github.com/s4mpp/backline",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "s4mpp",
            "email": "s4mpp.dev2@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": ">=11.0",
        "s4mpp/laravel-auth-suite": "^1.0",
        "s4mpp/blix-ui": "dev-master"
    },
    "require-dev": {
        "laravel/pint": "^1.26",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.12",
        "s4mpp/laragenius": "^2.0"
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/s4mpp/laravel-auth-suite"
        },
        {
            "type": "github",
            "url": "https://github.com/s4mpp/laragenius"
        },
        {
            "type": "github",
            "url": "https://github.com/s4mpp/blix-ui"
        }
    ],
    "autoload": {
        "psr-4": {
            "S4mpp\\Backline\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "S4mpp\\Backline\\Tests\\": "tests",
            "Workbench\\App\\": "workbench/app/",
            "Database\\Factories\\": "workbench/database/factories/",
            "Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "publish": "@php vendor/bin/testbench vendor:publish --tag=backline-assets",
        "create-config": [
            "cp workbench/config/backline.php vendor/orchestra/testbench-core/laravel/config"
        ],
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@create-config",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "S4mpp\\Backline\\BacklineServiceProvider"
            ]
        }
    }
}
